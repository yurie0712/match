<div class="clearfix">
  <% likes.each do |like| %>
    <div class="like">
      <%= link_to post_path(like.post.id) do %>
        <%= attachment_image_tag like.post, :image, :fill, 250, 250 %>
      <% end %>
      <ul class="inline-meta">
        <li>
          <%= link_to edit_like_path(like) do %>
            <ion-icon name="menu-outline"></ion-icon>
          <% end %>
        </li>
      </ul>
    </div>
  <% end %>
</div>
